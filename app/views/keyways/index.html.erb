<section id="cores">
	<div class="page-header">
		<h1>Listing keyways</h1>
	</div>

	<table>
	  <tr>
		<th>Name</th>
		<th></th>
		<th></th>
		<th></th>
	  </tr>

	<% @keyways.each do |keyway| %>
	  <tr>
		<td><%= keyway.name %></td>
		<td><%= link_to 'Show', keyway %></td>
		<td><%= link_to 'Edit', edit_keyway_path(keyway) %></td>
		<td><%= link_to 'Destroy', keyway, :confirm => 'Are you sure?', :method => :delete %></td>
	  </tr>
	<% end %>
	</table>

	<br />

	<%= link_to 'New Keyway', new_keyway_path %>
</section>