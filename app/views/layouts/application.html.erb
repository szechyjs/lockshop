<!doctype html>
<html>
<head>
  <title>SFIC Lock Shop</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div class="topbar">
		<div class="fill">
			<div class="container">
				<h3>
					<%= link_to 'SFIC Lockshop', '/' %>
				</h3>
				<ul>
					<li class>
						<%= link_to 'Keys', keys_path %>
					</li>
					<li class>
						<%= link_to 'Keyways', keyways_path %>
					</li>
					<li class>
						<%= link_to 'Key Types', keytypes_path %>
					</li>
					<li class>
						<%= link_to 'Cores', cores_path %>
					</li>
				</ul>
			</div>
		</div>
	</div>
	<div id="topspace"></div>
	<div class="container">
		<%= yield %>
	</div>
</body>
</html>
