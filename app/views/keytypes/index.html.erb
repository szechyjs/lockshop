<section id="cores">
	<div class="page-header">
		<h1>Listing keytypes</h1>
	</div>

	<table>
	  <tr>
		<th>Name</th>
		<th>Description</th>
		<th></th>
		<th></th>
		<th></th>
	  </tr>

	<% @keytypes.each do |keytype| %>
	  <tr>
		<td><%= keytype.name %></td>
		<td><%= keytype.description %></td>
		<td><%= link_to 'Show', keytype %></td>
		<td><%= link_to 'Edit', edit_keytype_path(keytype) %></td>
		<td><%= link_to 'Destroy', keytype, :confirm => 'Are you sure?', :method => :delete %></td>
	  </tr>
	<% end %>
	</table>

	<br />

	<%= link_to 'New Keytype', new_keytype_path %>
</section>